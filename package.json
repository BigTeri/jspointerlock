{
  "name": "jspointerlock",
  "version": "0.3.2",
  "description": "W3C Pointer Lock Fallback",
  "main": "dist/jspointerlock.js",
  "scripts": {
    "start": "http-server ./",
    "build": "jspm build [src/**/*.js] dist/jspointerlock.js",
    "build-docs": "jspm build index.js docs/index.js && cpy *.{html,css} docs/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ardean/jsPointerLock.git"
  },
  "author": "ardean",
  "license": "MIT",
  "devDependencies": {
    "cpy-cli": "^1.0.1",
    "http-server": "^0.9.0",
    "jspm": "^0.17.0-beta.38"
  },
  "jspm": {
    "name": "jspointerlock",
    "main": "src/index.js",
    "dependencies": {
      "events": "github:jspm/nodelibs-events@^0.1.1",
      "jquery": "npm:jquery@^3.1.1"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.20"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0"
    },
    "overrides": {
      "npm:jquery@3.1.1": {
        "format": "amd"
      }
    }
  }
}
